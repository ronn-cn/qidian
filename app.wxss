.container {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;

  display: flex;
  flex-direction: column; 
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
} 

page {
  /* background-image: linear-gradient(-45deg,#f5f5f7 50%, #eafafc, #dcebfd); */
  background-image: linear-gradient(-45deg,#f5f5f7 50%, #e0fcfc, #dbeafd);
  background-size: 100% 830px;
  font-size: 28rpx;
}

.element-hide{
  display: none;
}