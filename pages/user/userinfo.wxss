page{
  background-image: linear-gradient(-45deg,#f5f5f5 50%, #e0fcfc, #dbeafd); 
  background-size: 100% 1200px;
}

.container{
  width: 100%;
  padding: 0 20rpx;
  box-sizing: border-box;
  color: #465677;
}

.footer{
  margin: 10rpx auto;
  /* padding-bottom: 30rpx; */
  width: 90%;
  text-align: center;
  color: #8696bb;
}

.van-cell-group{
  background-color: transparent !important;
}

.van-cell{
  color: #465677 !important;
  background-color: transparent !important;
}

.van-field__label{
  color: #465677 !important;
}

.van-field__label--disabled{
  color: #465677 !important;
}

.van-tabbar{
  background-color: transparent !important;
}

.van-nav-bar{
  background-image: linear-gradient(-45deg,#f5f5f5 50%, #e0fcfc, #dbeafd) !important;
  background-size: 100% 1200px;
}